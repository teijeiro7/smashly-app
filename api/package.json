{
  "name": "smashly-api",
  "version": "1.0.0",
  "description": "API REST para Smashly - Sistema de Recomendación de Palas de Pádel con IA",
  "main": "dist/server.js",
  "scripts": {
    "setup": "node setup.js",
    "dev": "tsx watch src/server.ts",
